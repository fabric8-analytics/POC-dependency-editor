{
  "name": "fabric8-analytics-dependency-editor-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "e2e": "ng e2e",
    "clean": "npm run clean:lib && npm run clean:app",
    "clean:lib": "npm run rimraf -- projects/fabric8-analytics-dependency-editor/node_modules",
    "clean:app": "npm run rimraf -- node_modules dist",
    "build": "npm run build:lib && npm run build:app",
    "build:app": "ng build --output-hashing=all",
    "rimraf": "rimraf",
    "reinstall": "npm cache verify && npm install",
    "test": "npm run test:lib",
    "test:app": "ng test --watch=false",
    "test:lib": "ng test fabric8-analytics-dependency-editor --watch=false --browsers ChromeHeadlessNoSandbox",
    "test:lib:dev": "ng test fabric8-analytics-dependency-editor",
    "lint": "ng lint",
    "lint:app:fix": "ng lint fabric8-analytics-dependency-editor --fix",
    "lint:lib:fix": "ng lint fabric8-analytics-dependency-editor --fix",
    "lint:fix": "npm run lint:app:fix && npm run lint:lib:fix",
    "install:lib": "cd projects/fabric8-analytics-dependency-editor && npm i",
    "prebuild:lib": "npm run install:lib",
    "prebuild:app": "replace '%OSIO_AUTH_TOKEN%' $OSIO_AUTH_TOKEN src/environments/environment.ts",
    "clean:env": "git checkout src/environments/environment.ts",
    "build:lib": "ng build --prod fabric8-analytics-dependency-editor",
    "pack:lib": "cd dist/fabric8-analytics-dependency-editor && npm pack",
    "package": "npm run build:lib && npm run pack:lib",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric8-analytics/fabric8-analytics-dependency-editor.git"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26",
    "c3": "^0.4.18",
    "patternfly": "^3.28.0",
    "ngx-bootstrap": "^3.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "~6.1.3",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "protractor": "~5.3.0",
    "replace": "^1.0.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.9",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "release": {
    "branch": "master",
    "debug": "true",
    "pkgRoot": "dist/fabric8-analytics-dependency-editor"
  }
}
